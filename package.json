{
  "name": "warmeleads-distribution-system",
  "version": "1.0.0",
  "description": "Intelligent leads distribution system for Warmeleads.eu",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "leads",
    "distribution",
    "facebook-ads",
    "google-sheets"
  ],
  "author": "Warmeleads.eu",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "react-router-dom": "^6.30.1",
    "sequelize-cli": "^6.6.3",
    "web-vitals": "^5.0.3"
  }
}
